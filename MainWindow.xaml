<Window x:Class="Prog_124_S23_Midterm_Review_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prog_124_S23_Midterm_Review_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas Background="#FF9770EF">
        <Button x:Name="btnAddNewVehicle" Content="Add New Vehicle" Canvas.Left="288" Canvas.Top="373" Height="31" Width="157" Click="btnAddNewVehicle_Click"/>
        <ComboBox x:Name="cbVehicle" Canvas.Left="400" Canvas.Top="32" Width="340" HorizontalAlignment="Left" VerticalAlignment="Center" SelectionChanged="cbVehicle_SelectionChanged"/>
        <TextBox x:Name="txtType" Canvas.Left="510" TextWrapping="Wrap" Canvas.Top="79" Width="120"/>
        <TextBox x:Name="txtMake" Canvas.Left="510" TextWrapping="Wrap" Canvas.Top="120" Width="120" HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Canvas>
</Window>
